{
  "name": "universal-nestjs-auth-package",
  "version": "1.2.6",
  "description": "A comprehensive authentication package for NestJS applications providing JWT and session-based authentication with hybrid mode support, multi-device sessions, Redis integration, and Google OAuth.",
  "author": "mohammed_aljezawi",
  "license": "MIT",
  "main": "dist",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "keywords": [
    "nestjs",
    "authentication",
    "jwt",
    "session",
    "oauth",
    "microservices",
    "universal",
    "redis",
    "multi-device",
    "hybrid-auth",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/universal-nestjs-auth-package.git"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.0",
    "@types/crypto-js": "^4.2.2",
    "@types/express-session": "^1.18.2",
    "@types/ua-parser-js": "^0.7.39",
    "cache-manager": "^7.0.1",
    "connect-redis": "^9.0.0",
    "crypto-js": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "ua-parser-js": "^2.0.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.18",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
